rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // DİKKAT: Bu kural geçici bir çözümdür ve güvenli değildir.
    // Sadece geliştirme aşamasında tüm hataları ayıklamak için kullanılır.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
